
<div class="users-wrapper">
  <ul class="user-list">
    <% @users.each do |user| %>
        <li class="user-list-item">
          <a href=<%= user_path(user.id)%>>
            <%= image_tag(get_user_image_url(user)) %>
            <span><%= user.name %></span>
          </a>
        </li>
    <% end %>
  </ul>
</div>